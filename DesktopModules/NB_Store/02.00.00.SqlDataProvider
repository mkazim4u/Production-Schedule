	[CacheKey] [nvarchar] (50)  NULL ,
	[PortalID] [int] NULL ,
	[RoleName] [nvarchar] (50)  NULL ,
	[CategoryID] [int] NULL ,
	[ModelID] [int] NULL ,
	[SalePrice] [decimal](16, 2) NULL ,
	CONSTRAINT [PK_NB_Store_SaleRates] PRIMARY KEY  CLUSTERED 
	(
		[ItemID]
	)  ON [PRIMARY] 
) ON [PRIMARY]
GO




